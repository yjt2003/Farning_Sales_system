n      type: String,\n    },\n    description: {\n      type: String,\n    },\n    iconClass:{\n      type:String\n    },\n    img:{\n      type: String,\n      required: true,\n    }\n  },\n}\n</script>\n\n<style scoped>\n\n.little-logo{\n  position: absolute;\n  top: 30%;\n  left: 30%;\n  color: #97ff92;\n  font-size: 130px;\n}\n\n.title{\n  position: absolute;\n  top: 60%;\n  left: 25%;\n  font-size: 40px;\n  color: #97ff92;\n  font-family: 微软雅黑;\n  font-weight: bold;\n}\n\n.description{\n  position: absolute;\n  top: 40%;\n  left: 5%;\n  height: 200px;\n  padding: 1px 15px;\n  color: #2a2a2a;\n  font-size: 20px;\n  font-weight: bold;\n}\n\n/* entire container, keeps perspective */\n.flip-container {\n  perspective: 1000px;\n}\n/* flip the pane when hovered */\n.flip-container:hover .flipper, .flip-container.hover .flipper {\n  animation: flip 1s forwards;\n}\n\n\n\n\n.flip-container, .front, .back {\n  width: 320px;\n  height: 510px;\n}\n\n/* flip speed goes here */\n.flipper {\n  /*transition: 0.6s;*/\n  transform-style: preserve-3d;\n  /*position: relative;*/\n}\n\n/* hide back of pane during swap */\n.front, .back {\n  backface-visibility: hidden;\n  position: absolute;\n  top: 0;\n  left: 0;\n}\n\n/* front pane, placed above back */\n.front {\n  z-index: 2;\n}\n\nimg{\n  box-shadow: 0px 0px 10px black;\n}\n\n/* back, initially hidden pane */\n.back {\n  transform: rotateY(180deg);\n}\n\n@keyframes flip{\n  0% {\n    transform: rotateY(0deg) scale(1);\n  }\n  50% {\n    transform: rotateY(90deg) scale(1.2);\n  }\n  100% {\n    transform: rotateY(180deg) scale(1);\n  }\n}\n\n</style>\n"]}]}