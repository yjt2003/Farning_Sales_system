t(res.message);\r\n          } else {\r\n            alert(res.data);\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          alert(\"修改失败\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.changemessage {\r\n  .modify {\r\n    width: 40px;\r\n    height: 25px;\r\n    cursor: pointer;\r\n    margin-right: 10px;\r\n    color: #67C23A;\r\n    &:hover{\r\n      color: #035D1C;\r\n    }\r\n  }\r\n  .el-dialog {\r\n    width: 600px;\r\n    .order-img {\r\n      width: 148px;\r\n      height: 148px;\r\n      border-radius: 6px;\r\n    }\r\n    .orders-img_el_upload {\r\n      height: 148px;\r\n      .el-upload {\r\n        //   width: 50px;\r\n        //   height: 20px;\r\n        border: 1px dashed #d9d9d9;\r\n        border-radius: 6px;\r\n        cursor: pointer;\r\n        position: relative;\r\n        overflow: hidden;\r\n        &:hover {\r\n          border-color: #409eff;\r\n        }\r\n        .el-upload__input {\r\n          position: absolute;\r\n          left: -1000px;\r\n        }\r\n      }\r\n    }\r\n    .title {\r\n      width: 400px;\r\n    }\r\n    .content {\r\n      width: 400px;\r\n      height: 150px;\r\n      resize: none;\r\n      outline: none;\r\n      line-height: 1.5;\r\n      border: 1px solid #dcdfe6;\r\n    }\r\n    .price {\r\n      width: 100px;\r\n    }\r\n  } // width: 1200px;\r\n}\r\n</style>"]}]}