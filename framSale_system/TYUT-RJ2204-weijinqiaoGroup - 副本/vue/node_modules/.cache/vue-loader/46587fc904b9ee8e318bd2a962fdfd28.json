  </el-form>\n    <div style=\"text-align:center;margin-right:130px;\">\n      <el-button type=\"success\" @click=\"addExpertInfo\">修改</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getExpertInfo ,editExpertInfo} from '../api/expert.js'\n\nexport default {\n  data(){\n    return{\n      form:{\n        realName:'',\n        phone:'',\n        profession:'',\n        position:'',\n        belong:''\n      }\n    }\n  },\n  methods:{\n    getExpertData(){\n      getExpertInfo({}).then(res=>{\n        this.form = Object.assign({},{...res.data})\n      }).catch(err=>{\n        console.log(err)\n      })\n    },\n    addExpertInfo(){\n      editExpertInfo(this.form).then(res => {\n        console.log('edit',res)\n        if(res.flag==true){\n          this.$message.success('修改成功')\n        }else{\n          this.$message.error(res.message)\n        }\n      }).catch(err=>{\n        console.log(err)\n      })\n    }\n  },\n  mounted(){\n    this.getExpertData()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.expert-info-container{\n  width: 900px;\n  height: 100%;\n  background: #fff;\n  padding: 10px 20px;\n}\n</style>"]}]}