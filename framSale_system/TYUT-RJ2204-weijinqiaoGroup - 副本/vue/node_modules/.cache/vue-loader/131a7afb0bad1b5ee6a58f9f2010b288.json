>\r\n        <div class=\"item-text\">{{form.repayment}}(单位：月)</div>\r\n      </div>\r\n      <div class=\"item-content\">\r\n        <div class=\"item-title\">联系方式：</div>\r\n        <div class=\"item-text\">{{form.bankPhone}}</div>\r\n      </div>\r\n    </div>\r\n    <div class=\"btn-style\">\r\n      <el-button type=\"danger\" @click=\"handleApply\">申请</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { addFinance } from \"../api/finance\";\r\nimport ChangeMessage from \"../components/ChangeMessage.vue\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      data: [],\r\n      ownerInfo: {},\r\n      userGoods: [],\r\n      updateGoodInfo: {},\r\n      form:{\r\n        rate:'',\r\n        money:'',\r\n        bankPhone:'',\r\n        repayment:'',\r\n        status:0\r\n      }\r\n    };\r\n  },\r\n  filters: {\r\n    changeTime(time) {\r\n      return time.slice(0, 10);\r\n    },\r\n  },\r\n  components: { ChangeMessage },\r\n  props: {\r\n    ctype: {\r\n      type: String,\r\n    },\r\n    cdesciibe: {\r\n      type: String,\r\n    },\r\n  },\r\n  methods: {\r\n    handleApply(){\r\n      if(localStorage.getItem('token')){\r\n        this.form.status = 0\r\n        addFinance(this.form).then(res => {\r\n          if(res.flag==true){\r\n            this.$message.success(res.message)\r\n            this.$router.push(\"/home/financing\").catch((err) => err);\r\n            localStorage.removeItem('financeObj')\r\n          }else{\r\n            this.$message.error(res.message);\r\n          }\r\n        }).catch(err=>{\r\n          console.log(err)\r\n          this.$message.error(err.message)\r\n        })\r\n      }else{\r\n        this.$message.error('请先登录')\r\n        this.$router.push(`/login`).catch((err) => err);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.form = Object.assign({},{...JSON.parse(localStorage.getItem('financeObj'))})\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.details-box2