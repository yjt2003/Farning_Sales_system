menu-item index=\"1\" class=\"item\" @click=\"goodsBtn\"\r\n        >商品货源</el-menu-item\r\n      >\r\n      <el-menu-item index=\"2\" class=\"item\" @click=\"purchaseBtn\"\r\n        >求购需求</el-menu-item\r\n      >\r\n      <el-menu-item index=\"3\" class=\"item\" @click=\"knowledgesBtn\"\r\n        >知识讨论</el-menu-item\r\n      >\r\n      <el-menu-item index=\"4\" class=\"item\" @click=\"shopCartBtn\"\r\n        >购物车</el-menu-item\r\n      >\r\n      <span class=\"user\">\r\n        <el-col :span=\"12\"> </el-col>\r\n        <div class=\"userin\" v-if=\"$store.state.loginUserNickname == ''\">\r\n          <a href=\"javascript:;\" @click=\"Login\" class=\"login\">登录</a> |\r\n          <a href=\"javascript:;\" @click=\"Register\" class=\"register\">注册</a>\r\n        </div>\r\n        <div class=\"userout\" v-else>\r\n          <a href=\"javascript:;\" class=\"userinfo\"\r\n            ><img\r\n              :src=\"\r\n                $store.state.imgShowRoad + '/file/' + $store.state.loginUserAvatar\r\n              \"\r\n              alt=\"\"\r\n            />\r\n            <span>{{ $store.state.loginUserNickname }}</span>\r\n          </a>\r\n          <el-dropdown>\r\n            <span class=\"el-dropdown-link menu\">\r\n              <i class=\"el-icon-menu\"></i>\r\n            </span>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item icon=\"el-icon-user\" @click.native=\"userPage\"\r\n                >个人中心</el-dropdown-item\r\n              >\r\n              <el-dropdown-item\r\n                icon=\"el-icon-circle-plus\"\r\n                @click.native=\"addMessagePage\"\r\n                >发布信息</el-dropdown-item\r\n              >\r\n              <el-dropdown-item icon=\"el-icon-phone\">联系客服</el-dropdown-item>\r\n              <el-dropdown-item icon=\"el-icon-s-promotion\"\r\n                >意见反馈</el-dropdown-item\r\n              >\r\n              <el-dropdown-item icon=\"el-icon-info\">关于我们</el-dropdown-item>\r\n              <el-dropdown-item icon=\"el-icon-info\" @click.native=\"logout\"\r\n                >退出</el-dropdown-item\r\n              >\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </div>\r\n      </span>\r\n    </el-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nlet Base64 = require(\"js-base64\");\r\nexport default {\r\n  data() {\r\n    return {\r\n      avatar: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    handleSelect(key, keyPath) {},\r\n    Login() {\r\n      this.$router.push(\"/login\");\r\n    },\r\n    Register() {\r\n      this.$router.push(\"/register\");\r\n    },\r\n    logout() {\r\n      this.$store.commit(\"updateLoginUserNickname\", \"\");\r\n      this.$store.commit(\"removeStorage\");\r\n      this.$router.push(\"/home\").catch((err) => err);\r\n      if (sessionStorage.getItem(\"/order/needs/pageCode\")) {\r\n        sessionStorage.removeItem(\"/order/needs/pageCode\");\r\n      }\r\n      if (sessionStorage.getItem(\"/order/goods/pageCode\")) {\r\n        sessionStorage.removeItem(\"/order/goods/pageCode\");\r\n      }\r\n      if (sessionStorage.getItem(\"/user/search/pageCode\")) {\r\n        sessionStorage.removeItem(\"/user/search/pageCode\");\r\n      }\r\n    },\r\n    goodsBtn() {\r\n      this.$router.push(\"/home/goods\").catch((err) => err);\r\n    },\r\n    purchaseBtn() {\r\n      this.$router.push(\"/home/purchase\").catch((err) => err);\r\n    },\r\n    knowledgesBtn() {\r\n      this.$router.push(\"/home/knowledge\").catch((err) => err);\r\n    },\r\n    shopCartBtn() {\r\n      this.$router.push(\"/home/shopcart\").catch((err) => err);\r\n    },\r\n    userPage() {\r\n      this.$router.push(\"/home/user\").catch((err) => err);\r\n    },\r\n    addMessagePage() {\r\n      this.$router.push(\"/home/addmessage\").catch((err) => err);\r\n    },\r\n  },\r\n  created() {\r\n    this.$store.commit(\"updateActiveIndex\", \"1\");\r\n    if (window.localStorage.token) {\r\n      let token = window.localStorage.token;\r\n      let arr = token.split(\".\");\r\n      let res = Base64.decode(arr[1]);\r\n      this.$store.commit(\"updateLoginUserNickname\", JSON.parse(res).nickname);\r\n      this.$store.commit(\"updateLoginUserAvatar\", JSON.parse(res).avatar);\r\n      this.$store.commit(\"updateRole\", JSON.parse(res).role);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.navigation-bar {\r\n  .el-menu {\r\n    height: 80px;\r\n    background-color: #006e5e !important;\r\n    .el-menu-item {\r\n      background-color: #006e5e !important;\r\n      height: 79px;\r\n      line-height: 80px;\r\n      font-size: 16px;\r\n    }\r\n  }\r\n  .user {\r\n    float: right;\r\n    color: white;\r\n    line-height: 80px;\r\n    font-size: 16px;\r\n    margin-right: 15px;\r\n    .el-col {\r\n      width: 70px;\r\n      .el-dropdown-link {\r\n        font-size: 16px;\r\n        color: white;\r\n      }\r\n    }\r\n\r\n    .userin,\r\n    .userout {\r\n      height: 80px;\r\n      float: right;\r\n      text-align: right;\r\n      vertical-align: middle;\r\n      a {\r\n        display: inline-block;\r\n        box-sizing: border-box;\r\n        height: 80px;\r\n        text-decoration: none;\r\n        color: white;\r\n        img {\r\n          display: inline-block;\r\n          margin: 0 5px 5px 0;\r\n          width: 40px;\r\n          height: 40px;\r\n          border-radius: 50%;\r\n          background-color: pink;\r\n        }\r\n        // span {\r\n        //   display: inline-block;\r\n        //   width: 40px;\r\n        //   height: 20px;\r\n        //   margin-bottom: 5px;\r\n        // }\r\n      }\r\n      .el-dropdown {\r\n        height: 80px;\r\n        margin-left: 5px;\r\n        .menu {\r\n          color: white;\r\n          font-size: 20px;\r\n\r\n          .el-icon-menu {\r\n            line-height: 80px;\r\n            // display: inline-block;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]}]}