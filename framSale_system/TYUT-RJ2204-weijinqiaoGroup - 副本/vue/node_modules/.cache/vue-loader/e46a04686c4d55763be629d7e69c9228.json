A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SmartMatchUser.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"add-address\">\n    <div class=\"title\">\n      <el-button\n        type=\"success\"\n        plain round\n        style=\"margin-top: 10px\"\n        v-if=\"allIntentionData != ''\"\n        @click=\"lookIntention\"\n        >我的融资意向</el-button\n      >\n    </div>\n    <div v-if=\"look === true\">\n      <div\n        v-for=\"(item, index) in allIntentionData\"\n        :key=\"index\"\n        class=\"default-address-container\"\n      >\n        <div>\n          <div class=\"address-item\">姓名：{{ item.realName }}</div>\n          <div class=\"address-item\">地址：{{ item.address }}</div>\n          <div class=\"address-item\">联系方式：{{ item.phone }}</div>\n          <div class=\"address-item\">金额：{{ item.amount }} (单位：元)</div>\n          <div class=\"address-item\">用途：{{ item.application }}</div>\n          <div class=\"address-item\">农作物名称：{{ item.item }}</div>\n          <div class=\"address-item\">种植面积：{{ item.area }} (单位：亩)</div>\n          <div class=\"address-item\">\n            意向借款期：{{ item.repaymentPeriod }} (单位：月)\n          </div>\n        </div>\n        <div class=\"right-btn\">\n          <div\n            class=\"marginR20\"\n            v-if=\"allIntentionData != ''\"\n            @click=\"handleEdit(item)\"\n          >\n            编辑\n          </div>\n          <div\n            class=\"marginR20\"\n            v-if=\"allIntentionData != ''\"\n            @click=\"handleDel(item)\"\n          >\n            删除\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <el-button\n      type=\"success\" plain round\n      style=\"margin-top: 10px\"\n      v-if=\"allIntentionData == ''\"\n      @click=\"handleAdd\"\n      >添加融资意向</el-button\n    >\n\n    <el-dialog\n      :title=\"title\"\n      :visible.sync=\"showAdd\"\n      width=\"580px\"\n      :before-close=\"closeAdd\"\n    >\n      <el-form\n        :model=\"intentionData\"\n        ref=\"ruleForm\"\n        label-width=\"120px\"\n        class=\"demo-ruleForm\"\n      >\n        <el-form-item label=\"姓名：\" prop=\"realName\">\n          <el-input v-model=\"intentionData.realName\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"金额：\"  prop=\"amount\">\n          <el-input placeholder=\"单位：元\" v-model=\"intentionData.amount\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"联系方式：\" prop=\"phone\">\n          <el-input v-model=\"intentionData.phone\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"用途：\" prop=\"application\">\n          <el-input v-model=\"intentionData.application\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"农作物名称：\" prop=\"item\">\n          <el-input v-model=\"intentionData.item\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"种植面积：\" prop=\"area\">\n          <el-input  placeholder=\"单位：亩\" v-model=\"intentionData.area\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"意向借款期：\" prop=\"repaymentPeriod\">\n          <el-select v-model=\"value\" placeholder=\"意向借款期：\">\n            <el-option\n              v-for=\"item in options\"\n              :key=\"item.value\"\n              :label=\"item.label\"\n              :value=\"item.value\"\n            >\n            </el-option>\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"住址：\" prop=\"address\">\n          <el-input v-model=\"intentionData.address\"></el-input>\n        </el-form-item>\n      </el-form>\n\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeAdd\">取 消</el-button>\n        <el-button type=\"success\" @click=\"updateIntention\">确 定</el-button>\n      </span>\n    </el-dialog>\n\n    <div class=\"title\">\n      <br />\n      <strong>推荐融资人</strong>\n    </div>\n\n    <div class=\"goods-box\">\n      <div\n        class=\"goods\"\n        v-for=\"(item, index) in allRecommendData\"\n        :key=\"index\"\n        @click=\"detailsClick(item.userName)\"\n        :style=\"'margin-right:10px;'\"\n      >\n        <img\n          class=\"goods-img\"\n          v-if=\"item.avatar != ''\"\n          :src=\"$store.state.imgShowRoad + '/file/' + item.avatar\"\n          alt=\"\"\n        />\n        <img\n          class=\"goods-img\"\n          v-else\n          src=\"../assets/img/wutu.gif\"\n          style=\"border: 1px solid #f2f2f2\"\n        />\n\n        <div class=\"info\">\n          <span class=\"initiator1\" @click=\"detailsClick(item.userName)\"\n            >姓名：{{ item.realName }}</span\n          ><br />\n          <span class=\"initiator2\" @click=\"detailsClick(item.userName)\"\n            >联系方式：{{ item.phone }}</span\n          ><br />\n          <span class=\"initiator3\" @click=\"detailsClick(item.userName)\"\n            >地址：{{ item.address }}</span\n          ><br />\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\"\n            >农作物：{{ item.item }}</span\n          ><br />\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-if=\"item.amount!=-1\">金额：{{ item.amount }}元</span>\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-else>金额：- 元</span><br />\n          <!-- <span v-if=\"item.amount!=-1\">金额：{{ item.amount }}元</span>\n          <span v-else>金额：-元</span><br /> -->\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-if=\"item.area!=-1\"\n            >种植面积：{{ item.area }} 亩</span\n          >\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-else\n            >种植面积：- 亩</span\n          >\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  selectIntention,\n  insertIntention,\n  updateIntention,\n  deleteIntention,\n  selectRecommned,\n} from \"../api/finance\";\nimport ChangeMessage from \"../components/ChangeMessage.vue\";\nexport default {\n  inject: [\"reload\"],\n  data() {\n    return {\n      options: [\n        {\n          value: 6,\n          label: \"六个月\",\n        },\n        {\n          value: 12,\n          label: \"一年\",\n        },\n        {\n          value: 24,\n          label: \"两年\",\n        },\n        {\n          value: 36,\n          label: \"三年\",\n        },\n      ],\n      value: \"\",\n      intentionData: {\n        realName: \"\",\n        amount: \"\",\n        application: \"\",\n        item: \"\",\n        repaymentPeriod: \"\",\n        address: \"\",\n        area: \"\",\n        phone: \"\",\n      },\n      allIntentionData: [],\n      allRecommendData: [],\n      showAdd: false,\n      look: false,\n      title: \"添加融资意向\",\n    };\n  },\n  filters: {\n    changeTime(time) {\n      return time.slice(0, 10);\n    },\n  },\n  components: { ChangeMessage },\n  props: {\n    ctype: {\n      type: String,\n    },\n    cdesciibe: {\n      type: String,\n    },\n  },\n  methods: {\n    getAllRecommned() {\n      selectRecommned()\n        .then((res) => {\n          console.log(\"ressss\", res);\n          this.allRecommendData = res.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    getAllIntention() {\n      selectIntention()\n        .then((res) => {\n          console.log(\"ressss\", res);\n          this.allIntentionData = res.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    handleAdd() {\n      this.showAdd = true;\n    },\n    lookIntention() {\n      if (this.look === true) {\n        this.look = false;\n      } else {\n        this.look = true;\n      }\n    },\n    closeAdd() {\n      this.showAdd = false;\n      this.intentionData = {};\n    },\n    updateIntention() {\n      if (this.intentionData.amount == \"\") {\n        alert(\"金额不能为空\");\n        return;\n      } else if (this.intentionData.application == \"\") {\n        alert(\"用途不能为空\");\n        return;\n      } else if (this.intentionData.phone == \"\") {\n        alert(\"联系方式不能为空\");\n        return;\n      } else if (this.intentionData.item == \"\") {\n        alert(\"农作物名称不能为空\");\n        return;\n      } else if (this.intentionData.area == \"\") {\n        alert(\"种植面积不能为空\");\n        return;\n      } else if (this.value == \"\") {\n        alert(\"意向借款期不能为空\");\n        return;\n      } else if (this.intentionData.address == \"\") {\n        alert(\"住址不能为空\");\n        return;\n      }\n      if (this.title === \"编辑意向\") {\n        updateIntention({\n          realName: this.intentionData.realName,\n          amount: this.intentionData.amount,\n          application: this.intentionData.application,\n          item: this.intentionData.item,\n          repaymentPeriod: this.value,\n          address: this.intentionData.address,\n          area: this.intentionData.area,\n          phone: this.intentionData.phone,\n        }).then((res) => {\n          if (res.flag == true) {\n            this.getAllIntention();\n            this.$message.success(res.message);\n            this.showAdd = false;\n            this.getAllRecommned();\n          } else {\n            this.showAdd = false;\n            this.$message.error(res.data);\n          }\n        });\n      } else {\n        insertIntention({\n          realName: this.intentionData.realName,\n          amount: this.intentionData.amount,\n          application: this.intentionData.application,\n          item: this.intentionData.item,\n          repaymentPeriod: this.value,\n          address: this.intentionData.address,\n          area: this.intentionData.area,\n          phone: this.intentionData.phone,\n        }).then((res) => {\n          if (res.flag == true) {\n            this.getAllIntention();\n            this.$message.success(res.message);\n            this.getAllRecommned();\n            this.showAdd = false;\n          } else {\n            this.showAdd = false;\n            this.$message.error(res.data);\n          }\n        });\n      }\n    },\n    handleEdit(item) {\n      this.showAdd = true;\n      this.title = \"编辑意向\";\n      this.intentionData = Object.assign({}, { ...item });\n    },\n    handleDel(item) {\n      this.$confirm(\"确认删除该信息?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        // type: 'warning'\n      })\n        .then(() => {\n          deleteIntention({})\n            .then((res) => {\n              if (res.flag == true) {\n                this.reload();\n                // this.getAllIntention();\n                // this.getAllRecommned();\n                // this.intentionData=\n                this.$message({\n                  type: \"success\",\n                  message: \"删除成功!\",\n                });\n\n                // this.$router.push(\"/home/smartMatch\").catch((err) => err);\n              } else {\n                this.$message({ type: \"error\", message: res.message });\n              }\n            })\n            .catch((err) => {\n              this.$message({\n                type: \"error\",\n                message: \"删除失败!\",\n              });\n            });\n        })\n        .catch(() => {\n          this.$message({\n            type: \"info\",\n            message: \"已取消删除\",\n          });\n        });\n    },\n  },\n  created() {\n    this.getAllIntention();\n    this.getAllRecommned();\n    this.form = Object.assign(\n      {},\n      { ...JSON.parse(localStorage.getItem(\"financeObj\")) }\n    );\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n\n.add-address {\n  width: 1100px;\n  float: left;\n  padding: 20px;\n  background: #fff;\n  height: auto;\n  min-height: 100%;\n  .title {\n    border-bottom: 1px solid #f2f2f2;\n    padding: 10px 0px;\n    font-size: 18px;\n  }\n  .goods-box {\n    background-color: #f9f9f9;\n    width: 1100px;\n    margin: 0 auto;\n    .goods {\n      float: left;\n      text-align: center;\n      width: 200px;\n      height: 220px;\n      margin-top: 15px;\n      border-radius: 20%;\n      padding: 15px;\n      background-color: rgb(255, 255, 255);\n      border: 1px solid #d3d3d3;\n      position: relative;\n      // margin: 0 auto;\n      cursor: pointer;\n      .goods-img {\n        float: left;\n        width: 80px;\n        height: 80px;\n        margin-right: 10px;\n        border-radius: 100%;\n\n        position: absolute;\n        top: 10px;\n        left: 60px;\n      }\n      .info {\n        width: 180px;\n        top: 80px;\n        // float: left;\n        .initiator1 {\n          color: #666;\n          // position: absolute;top:80px;\n        }\n      }\n    }\n  }\n  .default-address-container {\n    border: 1px dashed #ccc;\n    border-radius: 6px;\n    padding: 10px 20px;\n    display: flex;\n    margin-top: 20px;\n    align-items: center;\n    justify-content: space-between;\n    .address-item {\n      line-height: 30px;\n      height: 30px;\n      margin-right: 50px;\n    }\n    .right-btn {\n      display: flex;\n      flex-direction: row;\n      justify-content: flex-start;\n      align-items: center;\n      .marginR20 {\n        margin-right: 20px;\n        cursor: pointer;\n        color: #67c23a;\n        &:hover {\n          color: #035d1c;\n          text-decoration: underline;\n        }\n      }\n    }\n  }\n}\n\n.details-box2 {\n  width: 1100px;\n  margin: 10px auto;\n  background: #fff;\n  min-height: 100%;\n  height: auto;\n\n  img {\n    width: 300px;\n    height: 300px;\n    float: left;\n    margin-right: 20px;\n  }\n  .title {\n    font-size: 18px;\n    line-height: 40px;\n  }\n  .introduce {\n    line-height: 25px;\n    max-height: 100px;\n  }\n  .item-content {\n    display: flex;\n    flex-direction: row;\n    justify-content: flex-start;\n    align-items: center;\n    height: 30px;\n    line-height: 30px;\n    .item-title {\n      font-weight: bold;\n    }\n    .item-text {\n    }\n  }\n  .info {\n    position: relative;\n    width: 340px;\n    height: 300px;\n    float: left;\n    .title {\n      font-size: 22px;\n      font-weight: bold;\n    }\n    .content {\n      height: 100px;\n    }\n    .price {\n      color: red;\n      font-size: 25px;\n      font-weight: bold;\n    }\n    .time {\n      margin-top: 10px;\n      color: #999;\n      .createtime {\n        float: left;\n      }\n      .updatetime {\n        float: right;\n      }\n    }\n  }\n  .operation {\n    position: absolute;\n    bottom: 0;\n    margin-top: 20px;\n    .like,\n    .collection,\n    .comment {\n      display: inline-block;\n      width: 30px;\n      margin-right: 30px;\n      img {\n        margin: 0;\n        width: 20px;\n        height: 20px;\n        border-radius: 6px;\n      }\n    }\n  }\n  .add-shopcart {\n    position: absolute;\n    right: 20px;\n    bottom: 5px;\n  }\n}\n</style>\n"]}]}