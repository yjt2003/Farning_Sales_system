IAAA,CAAA;AACA;AACA,SAdA,EAeA,KAfA,CAeA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,GAAA,CAAA;AACA,SAjBA;AAkBA;AACA;AA9BA,GARA;AAwCA,EAAA,OAxCA,qBAwCA,CAAA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"register-box\">\r\n    <div class=\"register\">\r\n      <div class=\"header\">\r\n        <h1>用户注册</h1>\r\n      </div>\r\n      <div id=\"register_form\">\r\n        <div class=\"form-group\">\r\n          <label for=\"username\">用户名</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"username\"\r\n            name=\"username\"\r\n            v-model=\"userName\"\r\n            placeholder=\"请输入用户名\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"nickname\">昵称</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"nickname\"\r\n            name=\"nickname\"\r\n            v-model=\"nickName\"\r\n            placeholder=\"请输入昵称\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">密码</label>\r\n          <input\r\n            type=\"password\"\r\n            class=\"form-control\"\r\n            id=\"password\"\r\n            name=\"password\"\r\n            v-model=\"password\"\r\n            placeholder=\"请输入密码\"\r\n          />\r\n        </div>\r\n        <button class=\"btn btn-success btn-block\" @click=\"registerBtn\">\r\n          注册\r\n        </button>\r\n        <!-- <input type=\"submit\" value=\"tijiao\" /> -->\r\n      </div>\r\n      <div class=\"message\">\r\n        <p>已有账号? <router-link to=\"/login\">点击登录</router-link>.</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { userRegister } from \"../api/user\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      userName: \"\",\r\n      password: \"\",\r\n      nickName: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    registerBtn() {\r\n      if (this.userName == \"\") {\r\n        alert(\"用户名不能为空\");\r\n        return;\r\n      } else if (this.nickName == \"\") {\r\n        alert(\"昵称不能为空\");\r\n      } else if (this.password == \"\") {\r\n        alert(\"密码不能为空\");\r\n        return;\r\n      } else {\r\n        userRegister({\r\n          userName: this.userName,\r\n          password: this.password,\r\n          nickName: this.nickName,\r\n          avatar: \"rongxiaotong.gif\",\r\n        })\r\n          .then((res) => {\r\n            if (res.flag == true) {\r\n              alert(res.message);\r\n              this.$store.commit(\"updateLoginUserNickname\", this.nickName);\r\n              this.$router.push(\"/login\").catch((err) => err);\r\n            } else {\r\n              alert(res.data);\r\n            }\r\n          })\r\n          .catch((err) => {\r\n            alert(err);\r\n          });\r\n      }\r\n    },\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import url(\"../../node_modules/bootstrap/dist/css/bootstrap.css\");\r\nbody {\r\n  background-color: #f9f9f9;\r\n}\r\n.register-box {\r\n  box-sizing: border-box;\r\n  // width: 1897px;\r\n  height: 800px;\r\n  padding-top: 150px;\r\n  background: url(\"../assets/img/rice.png\");\r\n  background-size: 1897px 920px;\r\n  .register {\r\n    width: 340px;\r\n    margin: 0 auto;\r\n    color: #333;\r\n    .header {\r\n      height: 40px;\r\n      text-align: center;\r\n      h1 {\r\n        margin: 0;\r\n        font-size: 26px;\r\n        color: white;\r\n      }\r\n    }\r\n    #register_form {\r\n      padding: 20px;\r\n      margin-bottom: 15px;\r\n      border: 1px solid #d8dee2;\r\n      border-radius: 5px;\r\n      background-color: #fff;\r\n    }\r\n    .message {\r\n      padding: 10px;\r\n      padding-bottom: 0;\r\n      color: white;\r\n      border: 1px solid #d8dee2;\r\n      border-radius: 5px;\r\n      text-align: center;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}