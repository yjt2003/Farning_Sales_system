ppableCard\",\r\n  props:{\r\n    title: {\r\n      type: String,\r\n    },\r\n    description: {\r\n      type: String,\r\n    },\r\n    iconClass:{\r\n      type:String\r\n    },\r\n    img:{\r\n      type: String,\r\n      required: true,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.little-logo{\r\n  position: absolute;\r\n  top: 30%;\r\n  left: 30%;\r\n  color: #97ff92;\r\n  font-size: 130px;\r\n}\r\n\r\n.title{\r\n  position: absolute;\r\n  top: 60%;\r\n  left: 25%;\r\n  font-size: 40px;\r\n  color: #97ff92;\r\n  font-family: 微软雅黑;\r\n  font-weight: bold;\r\n}\r\n\r\n.description{\r\n  position: absolute;\r\n  top: 40%;\r\n  left: 5%;\r\n  height: 200px;\r\n  padding: 1px 15px;\r\n  color: #2a2a2a;\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n}\r\n\r\n/* entire container, keeps perspective */\r\n.flip-container {\r\n  perspective: 1000px;\r\n}\r\n/* flip the pane when hovered */\r\n.flip-container:hover .flipper, .flip-container.hover .flipper {\r\n  animation: flip 1s forwards;\r\n}\r\n\r\n\r\n\r\n\r\n.flip-container, .front, .back {\r\n  width: 320px;\r\n  height: 510px;\r\n}\r\n\r\n/* flip speed goes here */\r\n.flipper {\r\n  /*transition: 0.6s;*/\r\n  transform-style: preserve-3d;\r\n  /*position: relative;*/\r\n}\r\n\r\n/* hide back of pane during swap */\r\n.front, .back {\r\n  backface-visibility: hidden;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n}\r\n\r\n/* front pane, placed above back */\r\n.front {\r\n  z-index: 2;\r\n}\r\n\r\nimg{\r\n  box-shadow: 0px 0px 10px black;\r\n}\r\n\r\n/* back, initially hidden pane */\r\n.back {\r\n  transform: rotateY(180deg);\r\n}\r\n\r\n@keyframes flip{\r\n  0% {\r\n    transform: rotateY(0deg) scale(1);\r\n  }\r\n  50% {\r\n    transform: rotateY(90deg) scale(1.2);\r\n  }\r\n  100% {\r\n    transform: rotateY(180deg) scale(1);\r\n  }\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}