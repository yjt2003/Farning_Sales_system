ted() {\n    this.$store.commit(\"updateActiveIndex\", \"2\");\n    this.getData()\n  },\n  methods: {\n    getData(){\n      selectGoodsPage({\n        pageNum: this.goodsCount,\n        keys:this.searchValue\n      }).then((res) => {\n        if (res.flag == true) {\n          this.goods = res.data.list;\n          this.total = res.data.total;\n        } else {\n          // alert(res.data.data);\n        }\n      });\n    },\n    pageClick(item) {\n      this.goods = item;\n    },\n    handleSearch(val){\n      this.searchValue = val\n      this.getData()\n    }\n  },\n  components: {\n    Pagination,\n    GoodsSourse,\n    Subtitle\n  },\n  // 监听,当路由发生变化的时候执行\n  watch: {\n    $route: {\n      handler: function(val, oldVal){\n        console.log(val);\n      },\n      // 深度观察监听\n      deep: true\n    }\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.home-goods {\n  width: 1100px;\n  margin: 0 auto;\n}\n\n.font-background{\n  background-image: url(\"../../assets/page2/background2.jpg\");\n  height: 400px;\n  width: 1920px;\n  padding-left: 350px;\n  padding-top: 90px;\n}\n\n\n</style>\n"],"sourceRoot":"src/views/GoodSourcePage"}]}