AA,KAAA,CAAA,QAAA,CAAA;AACA;AACA;;AACA,UAAA,KAAA,KAAA,KAAA,MAAA,EAAA;AACA,QAAA,gBAAA,CAAA;AACA,UAAA,QAAA,EAAA,KAAA,aAAA,CAAA,QADA;AAEA,UAAA,MAAA,EAAA,KAAA,aAAA,CAAA,MAFA;AAGA,UAAA,WAAA,EAAA,KAAA,aAAA,CAAA,WAHA;AAIA,UAAA,IAAA,EAAA,KAAA,aAAA,CAAA,IAJA;AAKA,UAAA,eAAA,EAAA,KAAA,KALA;AAMA,UAAA,OAAA,EAAA,KAAA,aAAA,CAAA,OANA;AAOA,UAAA,IAAA,EAAA,KAAA,aAAA,CAAA,IAPA;AAQA,UAAA,KAAA,EAAA,KAAA,aAAA,CAAA;AARA,SAAA,CAAA,CASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,YAAA,MAAA,CAAA,eAAA;AACA,WALA,MAKA;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA;AACA;AACA,SAnBA;AAoBA,OArBA,MAqBA;AACA,QAAA,eAAA,CAAA;AACA,UAAA,QAAA,EAAA,KAAA,aAAA,CAAA,QADA;AAEA,UAAA,MAAA,EAAA,KAAA,aAAA,CAAA,MAFA;AAGA,UAAA,WAAA,EAAA,KAAA,aAAA,CAAA,WAHA;AAIA,UAAA,IAAA,EAAA,KAAA,aAAA,CAAA,IAJA;AAKA,UAAA,eAAA,EAAA,KAAA,KALA;AAMA,UAAA,OAAA,EAAA,KAAA,aAAA,CAAA,OANA;AAOA,UAAA,IAAA,EAAA,KAAA,aAAA,CAAA,IAPA;AAQA,UAAA,KAAA,EAAA,KAAA,aAAA,CAAA;AARA,SAAA,CAAA,CASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,eAAA;;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,WALA,MAKA;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA;AACA;AACA,SAnBA;AAoBA;AACA,KArGA;AAsGA,IAAA,UAtGA,sBAsGA,IAtGA,EAsGA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,MAAA;AACA,WAAA,aAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,oBAAA,IAAA,EAAA;AACA,KA1GA;AA2GA,IAAA,SA3GA,qBA2GA,IA3GA,EA2GA;AAAA;;AACA,WAAA,QAAA,CAAA,UAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA,CAGA;;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,QAAA,eAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,GADA,CAEA;AACA;AACA;;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA;AACA,cAAA,IAAA,EAAA,SADA;AAEA,cAAA,OAAA,EAAA;AAFA,aAAA,EALA,CAUA;;AACA,WAXA,MAWA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA;AAAA,cAAA,IAAA,EAAA,OAAA;AAAA,cAAA,OAAA,EAAA,GAAA,CAAA;AAAA,aAAA;AACA;AACA,SAhBA,EAiBA,KAjBA,CAiBA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SAtBA;AAuBA,OA7BA,EA8BA,KA9BA,CA8BA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAnCA;AAoCA;AAhJA,GAtDA;AAwMA,EAAA,OAxMA,qBAwMA;AACA,SAAA,eAAA;AACA,SAAA,eAAA;AACA,SAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CACA,EADA,oBAEA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,YAAA,CAAA,CAFA,EAAA;AAIA;AA/MA,CAAA","sourcesContent":["<template>\r\n  <div class=\"add-address\">\r\n    <div class=\"title\">\r\n      <el-button\r\n        type=\"success\"\r\n        style=\"margin-top: 10px\"\r\n        v-if=\"allIntentionData != ''\"\r\n        @click=\"lookIntention\"\r\n        >我的融资意向</el-button\r\n      >\r\n    </div>\r\n    <div v-if=\"look === true\">\r\n      <div\r\n        v-for=\"(item, index) in allIntentionData\"\r\n        :key=\"index\"\r\n        class=\"default-address-container\"\r\n      >\r\n        <div>\r\n          <div class=\"address-item\">姓名：{{ item.realName }}</div>\r\n          <div class=\"address-item\">地址：{{ item.address }}</div>\r\n          <div class=\"address-item\">联系方式：{{ item.phone }}</div>\r\n          <div class=\"address-item\">金额：{{ item.amount }} (单位：元)</div>\r\n          <div class=\"address-item\">用途：{{ item.application }}</div>\r\n          <div class=\"address-item\">农作物名称：{{ item.item }}</div>\r\n          <div class=\"address-item\">种植面积：{{ item.area }} (单位：亩)</div>\r\n          <div class=\"address-item\">\r\n            意向借款期：{{ item.repaymentPeriod }} (单位：月)\r\n          </div>\r\n        </div>\r\n        <div class=\"right-btn\">\r\n          <div\r\n            class=\"marginR20\"\r\n            v-if=\"allIntentionData != ''\"\r\n            @click=\"handleEdit(item)\"\r\n          >\r\n            编辑\r\n          </div>\r\n          <div\r\n            class=\"marginR20\"\r\n            v-if=\"allIntentionData != ''\"\r\n            @click=\"handleDel(item)\"\r\n          >\r\n            删除\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <el-button\r\n      type=\"success\"\r\n      style=\"margin-top: 10px\"\r\n      v-if=\"allIntentionData == ''\"\r\n      @click=\"handleAdd\"\r\n      >添加融资意向</el-button\r\n    >\r\n\r\n    <el-dialog\r\n      :title=\"title\"\r\n      :visible.sync=\"showAdd\"\r\n      width=\"580px\"\r\n      :before-close=\"closeAdd\"\r\n    >\r\n      <el-form\r\n        :model=\"intentionData\"\r\n        ref=\"ruleForm\"\r\n        label-width=\"100px\"\r\n        class=\"demo-ruleForm\"\r\n      >\r\n        <el-form-item label=\"姓名：\" prop=\"realName\">\r\n          <el-input v-model=\"intentionData.realName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"金额(单位：元)：\" prop=\"amount\">\r\n          <el-input v-model=\"intentionData.amount\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系方式：\" prop=\"phone\">\r\n          <el-input v-model=\"intentionData.phone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用途：\" prop=\"application\">\r\n          <el-input v-model=\"intentionData.application\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"农作物名称：\" prop=\"item\">\r\n          <el-input v-model=\"intentionData.item\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"种植面积(单位：亩)：\" prop=\"area\">\r\n          <el-input v-model=\"intentionData.area\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"意向借款期：\" prop=\"repaymentPeriod\">\r\n          <el-select v-model=\"value\" placeholder=\"意向借款期：\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"住址：\" prop=\"address\">\r\n          <el-input v-model=\"intentionData.address\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"closeAdd\">取 消</el-button>\r\n        <el-button type=\"success\" @click=\"updateIntention\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <div class=\"title\">\r\n      <br />\r\n      <strong>推荐融资人</strong>\r\n    </div>\r\n    \r\n    <div class=\"goods-box\">\r\n      <div\r\n        class=\"goods\"\r\n        v-for=\"(item, index) in allRecommendData\"\r\n        :key=\"index\"\r\n        @click=\"detailsClick(item.userName)\"\r\n        :style=\"'margin-right:10px;'\"\r\n      >\r\n        <img\r\n          class=\"goods-img\"\r\n          v-if=\"item.avatar != ''\"\r\n          :src=\"$store.state.imgShowRoad + '/file/' + item.avatar\"\r\n          alt=\"\"\r\n        />\r\n        <img\r\n          class=\"goods-img\"\r\n          v-else\r\n          src=\"../assets/img/wutu.gif\"\r\n          style=\"border: 1px solid #f2f2f2\"\r\n        />\r\n\r\n        <div class=\"info\">\r\n          <span class=\"initiator1\" @click=\"detailsClick(item.userName)\"\r\n            >姓名：{{ item.realName }}</span\r\n          ><br />\r\n          <span class=\"initiator2\" @click=\"detailsClick(item.userName)\"\r\n            >联系方式：{{ item.phone }}</span\r\n          ><br />\r\n          <span class=\"initiator3\" @click=\"detailsClick(item.userName)\"\r\n            >地址：{{ item.address }}</span\r\n          ><br />\r\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\"\r\n            >农作物：{{ item.item }}</span\r\n          ><br />\r\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-if=\"item.amount!=-1\">金额：{{ item.amount }}元</span>\r\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-else>金额：- 元</span><br />\r\n          <!-- <span v-if=\"item.amount!=-1\">金额：{{ item.amount }}元</span>\r\n          <span v-else>金额：-元</span><br /> -->\r\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-if=\"item.area!=-1\"\r\n            >种植面积：{{ item.area }} 亩</span\r\n          >  \r\n          <span class=\"initiator4\" @click=\"detailsClick(item.userName)\" v-else\r\n            >种植面积：- 亩</span\r\n          > \r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  selectIntention,\r\n  insertIntention,\r\n  updateIntention,\r\n  deleteIntention,\r\n  selectRecommned,\r\n} from \"../api/finance\";\r\nimport ChangeMessage from \"../components/ChangeMessage.vue\";\r\nexport default {\r\n  inject: [\"reload\"],\r\n  data() {\r\n    return {\r\n      options: [\r\n        {\r\n          value: 6,\r\n          label: \"六个月\",\r\n        },\r\n        {\r\n          value: 12,\r\n          label: \"一年\",\r\n        },\r\n        {\r\n          value: 24,\r\n          label: \"两年\",\r\n        },\r\n        {\r\n          value: 36,\r\n          label: \"三年\",\r\n        },\r\n      ],\r\n      value: \"\",\r\n      intentionData: {\r\n        realName: \"\",\r\n        amount: \"\",\r\n        application: \"\",\r\n        item: \"\",\r\n        repaymentPeriod: \"\",\r\n        address: \"\",\r\n        area: \"\",\r\n        phone: \"\",\r\n      },\r\n      allIntentionData: [],\r\n      allRecommendData: [],\r\n      showAdd: false,\r\n      look: false,\r\n      title: \"添加融资意向\",\r\n    };\r\n  },\r\n  filters: {\r\n    changeTime(time) {\r\n      return time.slice(0, 10);\r\n    },\r\n  },\r\n  components: { ChangeMessage },\r\n  props: {\r\n    ctype: {\r\n      type: String,\r\n    },\r\n    cdesciibe: {\r\n      type: String,\r\n    },\r\n  },\r\n  methods: {\r\n    getAllRecommned() {\r\n      selectRecommned()\r\n        .then((res) => {\r\n          console.log(\"ressss\", res);\r\n          this.allRecommendData = res.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    getAllIntention() {\r\n      selectIntention()\r\n        .then((res) => {\r\n          console.log(\"ressss\", res);\r\n          this.allIntentionData = res.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    handleAdd() {\r\n      this.showAdd = true;\r\n    },\r\n    lookIntention() {\r\n      if (this.look === true) {\r\n        this.look = false;\r\n      } else {\r\n        this.look = true;\r\n      }\r\n    },\r\n    closeAdd() {\r\n      this.showAdd = false;\r\n      this.intentionData = {};\r\n    },\r\n    updateIntention() {\r\n      if (this.intentionData.amount == \"\") {\r\n        alert(\"金额不能为空\");\r\n        return;\r\n      } else if (this.intentionData.application == \"\") {\r\n        alert(\"用途不能为空\");\r\n        return;\r\n      } else if (this.intentionData.phone == \"\") {\r\n        alert(\"联系方式不能为空\");\r\n        return;\r\n      } else if (this.intentionData.item == \"\") {\r\n        alert(\"农作物名称不能为空\");\r\n        return;\r\n      } else if (this.intentionData.area == \"\") {\r\n        alert(\"种植面积不能为空\");\r\n        return;\r\n      } else if (this.value == \"\") {\r\n        alert(\"意向借款期不能为空\");\r\n        return;\r\n      } else if (this.intentionData.address == \"\") {\r\n        alert(\"住址不能为空\");\r\n        return;\r\n      }\r\n      if (this.title === \"编辑意向\") {\r\n        updateIntention({\r\n          realName: this.intentionData.realName,\r\n          amount: this.intentionData.amount,\r\n          application: this.intentionData.application,\r\n          item: this.intentionData.item,\r\n          repaymentPeriod: this.value,\r\n          address: this.intentionData.address,\r\n          area: this.intentionData.area,\r\n          phone: this.intentionData.phone,\r\n        }).then((res) => {\r\n          if (res.flag == true) {\r\n            this.getAllIntention();\r\n            this.$message.success(res.message);\r\n            this.showAdd = false;\r\n            this.getAllRecommned();\r\n          } else {\r\n            this.showAdd = false;\r\n            this.$message.error(res.data);\r\n          }\r\n        });\r\n      } else {\r\n        insertIntention({\r\n          realName: this.intentionData.realName,\r\n          amount: this.intentionData.amount,\r\n          application: this.intentionData.application,\r\n          item: this.intentionData.item,\r\n          repaymentPeriod: this.value,\r\n          address: this.intentionData.address,\r\n          area: this.intentionData.area,\r\n          phone: this.intentionData.phone,\r\n        }).then((res) => {\r\n          if (res.flag == true) {\r\n            this.getAllIntention();\r\n            this.$message.success(res.message);\r\n            this.getAllRecommned();\r\n            this.showAdd = false;\r\n          } else {\r\n            this.showAdd = false;\r\n            this.$message.error(res.data);\r\n          }\r\n        });\r\n      }\r\n    },\r\n    handleEdit(item) {\r\n      this.showAdd = true;\r\n      this.title = \"编辑意向\";\r\n      this.intentionData = Object.assign({}, { ...item });\r\n    },\r\n    handleDel(item) {\r\n      this.$confirm(\"确认删除该信息?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        // type: 'warning'\r\n      })\r\n        .then(() => {\r\n          deleteIntention({})\r\n            .then((res) => {\r\n              if (res.flag == true) {\r\n                this.reload();\r\n                // this.getAllIntention();\r\n                // this.getAllRecommned();\r\n                // this.intentionData=\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"删除成功!\",\r\n                