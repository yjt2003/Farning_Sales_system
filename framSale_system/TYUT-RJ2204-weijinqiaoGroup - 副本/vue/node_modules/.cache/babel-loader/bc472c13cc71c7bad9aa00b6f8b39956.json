oBAAA,CAAA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,GAAA;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OALA;AAMA,KAXA;AAYA,IAAA,WAZA,uBAYA,IAZA,EAYA;AAAA;;AACA,WAAA,QAAA,CAAA,WAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,mBAAA,CAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,OAAA;AACA,SANA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SARA;AASA,OAdA,EAcA,KAdA,CAcA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAnBA;AAoBA,KAjCA;AAkCA,IAAA,YAlCA,wBAkCA,IAlCA,EAkCA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,oBAAA,IAAA,EAAA;AACA,KArCA;AAsCA,IAAA,WAtCA,yBAsCA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KAxCA;AAyCA,IAAA,UAzCA,sBAyCA,IAzCA,EAyCA;AACA,UAAA,KAAA,IAAA,KAAA,YAAA,EAAA;AACA,aAAA,KAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,KAAA,GAAA,IAAA;AACA;;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,oBAAA,IAAA,EAAA;AACA,KAjDA;AAkDA,IAAA,WAlDA,yBAkDA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,KApDA;AAqDA,IAAA,YArDA,0BAqDA;AAAA;;AACA,UAAA,KAAA,IAAA,KAAA,QAAA,EAAA;AACA,aAAA,SAAA,CAAA,MAAA,GAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,SAAA,CAAA,MAAA,GAAA,CAAA;AACA;;AAEA,MAAA,sBAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA;AAnEA,GAzBA;AA8FA,EAAA,OA9FA,qBA8FA;AACA,SAAA,MAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,KAAA;AACA,SAAA,OAAA;AACA;AAjGA,CAAA","sourcesContent":["<!--专家问答-->\n<template>\n  <div class=\"expert-question-container\">\n    <div v-for=\"(item,index) in questionArray\" :key=\"index\" class=\"question-item\">\n      <div class=\"question-content\">\n        <div class=\"title\">{{item.title}}</div>\n        <div class=\"content\">{{item.question}}</div>\n        <div class=\"item-container\">\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">提问者：<span class=\"item-text\">{{item.questioner}}</span> </div>\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">提问者联系方式：<span class=\"item-text\">{{item.phone}}</span> </div>\n          <div class=\"item-content marginR30\" v-if=\"role==='questioner'\">专家姓名：<span class=\"item-text\">{{item.expertName}}</span></div>\n          <el-tag :type=\"item.status === 0 ? 'danger':'success'\" size=\"mini\">{{item.status === 0 ? '未回答' :'已回答'}}</el-tag>\n        </div>\n      </div>\n      <div class=\"question-btn\">\n        <div class=\"btn-text\" @click=\"handleDetail(item)\">详情</div>\n        <div class=\"btn-text\" @click=\"handleEdit(item)\" v-if=\"role==='questioner'\">修改</div>\n        <div class=\"btn-text\" @click=\"handleEdit(item)\" v-if=\"role==='expert'\">回答</div>\n        <div class=\"btn-text\" @click=\"delQuestion(item)\">删除</div>\n      </div>\n    </div>\n    <el-dialog title=\"详情\" :visible.sync=\"showDetail\" width=\"600px\" :before-close=\"detailClose\">\n      <div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题标题：</div>\n          <div class=\"item-content\">{{detailObj.title}}</div>\n        </div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题内容：</div>\n          <div class=\"item-content\">{{detailObj.question}}</div>\n        </div>\n        <div class=\"detail-item\" v-if=\"role==='questioner'\">\n          <div class=\"item-title\">专家姓名：</div>\n          <div class=\"item-content\">{{detailObj.expertName}}</div>\n        </div>\n        <!-- <div class=\"detail-item\">\n          <div class=\"item-title\">提问者：</div>\n          <div class=\"item-content\">{{detailObj.questioner}}</div>\n        </div> -->\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题状态：</div>\n          <el-tag type=\"danger\" size=\"mini\">{{detailObj.status === 0 ? '未回答' :'已回答'}}</el-tag>\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <!-- <el-button @click=\"showDetail = false\">取 消</el-button> -->\n        <el-button type=\"primary\" @click=\"showDetail = false\">确 定</el-button>\n      </span>\n    </el-dialog>\n    <el-dialog :title=\"title\" :visible.sync=\"dialogVisible\" width=\"650px\" :before-close=\"closeRevise\">\n      <div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题标题：</div>\n          <div class=\"item-content\">{{detailObj.title}}</div>\n        </div>\n        <!-- 专家不能改问题内容 -->\n        <div class=\"detail-item\" v-if=\"role==='expert'||detailObj.status === 1\">\n          <div class=\"item-title\">内容：</div>\n          <div class=\"item-content\">{{detailObj.question}}</div>\n        </div>\n        <!-- 提问者可以改问题内容 -->\n        <el-form ref=\"form\" :model=\"detailObj\" label-width=\"60px\" v-if=\"role==='questioner'&&detailObj.status === 0\">\n          <el-form-item label=\"内容：\">\n            <el-input type=\"textarea\" v-model=\"detailObj.question\"></el-input>\n          </el-form-item>\n        </el-form>\n\n        <div class=\"detail-item\" v-if=\"role==='questioner'\">\n          <div class=\"item-title\">回答：</div>\n          <div class=\"item-content\">{{detailObj.answer}}</div>\n        </div>\n        <el-form ref=\"form\" :model=\"detailObj\" label-width=\"60px\" v-if=\"role==='expert'\">\n          <el-form-item label=\"回答：\">\n            <el-input type=\"textarea\" v-model=\"detailObj.answer\"></el-input>\n          </el-form-item>\n        </el-form>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeRevise\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submitRevise\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { selectQuestionByUser,reviseQuestionByUserId,delQuestionByUserId } from '../api/question.js'\n\nexport default {\n  data(){\n    return{\n      questionArray:[],\n      showDetail: false,\n      dialogVisible: false,\n      role:\"\",\n      title:\"\",\n      detailObj:{\n        title:'',\n        question:'',\n        expertName:'',\n        answer:'',\n        address:'',\n        area:'',\n        expertName:'',\n        plantCondition:'',\n        plantDetail:'',\n        plantName:'',\n        questioner:'',\n        soilCondition:'',\n        status:1\n      }\n    }\n  },\n  methods:{\n    getData(){\n      this.role = this.$store.getters.isExpert?'expert':'questioner'\n      console.log('role',this.role)\n      // kind：普通用户：questioner；专家：expert\n      selectQuestionByUser({role:this.role}).then(res => {\n        console.log('rererer',res)\n        this.questionArray = res.data\n      }).catch(err=>{\n        console.log(err)\n      })\n    },\n    delQuestion(item){\n      this.$confirm('确认删除该行信息？', '删除', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        delQuestionByUserId({id:item.id}).then(res=>{\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          this.getData()\n        }).catch(err=>{\n          console.log(err)\n        })\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });          \n      });\n    },\n    handleDetail(item){\n      this.showDetail = true\n      this.detailObj = Object.assign({},{...item})\n    },\n    detailClose(){\n      this.showDetail = false\n    },\n    handleEdit(item){\n      if(this.role === 'questioner'){\n        this.title = \"修改\";\n      }else{\n        this.title = \"回答\";\n      }\n      this.dialogVisible = true\n      this.detailObj = Object.assign({},{...item})\n    },\n    closeRevise(){\n      this.dialogVisible = false\n    },\n    submitRevise(){\n      if(this.role === 'expert'){\n        this.detailObj.status = 1\n      }else{\n        this.detailObj.status = 0\n      }\n      \n      reviseQuestionByUserId(this.detailObj).then(res => {\n        this.$message.success('修改成功！')\n        this.dialogVisible = false\n        this.getData()\n      }).catch(err=>{\n        console.log(err)\n      })\n    }\n  },\n  mounted(){\n    this.$store.commit(\"updateUserActiveIndex\", \"4-1\");\n    this.getData()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.expert-question-container{\n  width: 900px;\n  min-height: 100%;\n  // padding: 10px 20px;\n  background: #fff;\n  .question-item{\n    border:1px solid #f2f2f2;\n    border-radius: 6px;\n    padding: 10px 20px;\n    margin: 10px 20px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    align-items: center;\n    .question-content{\n      width: 650px;\n      .title{\n        font-weight: bold;\n        font-size: 18px;\n        line-height: 30px;\n        height: 30px;\n      }\n      .content{\n        line-height: 25px;\n        height: auto;\n        word-break: break-all;\n      }\n      .item-container{\n        display: flex;\n        flex-direction: row;\n        justify-content: flex-start;\n        align-items: center;\n        font-size: 12px;\n        line-height: 20px;\n      }\n      .marginR30{\n        margin-right: 30px;\n      }\n      .item-text{\n        color: #666;\n      }\n    }\n    .question-btn{\n      display: flex;\n      .btn-text{\n        height: 25px;\n        cursor: pointer;\n        margin-right: 10px;\n        color: #67C23A;\n        &:hover{\n          color: #035D1C;\n          text-decoration: underline;\n        }\n      }\n    }\n  }\n  .detail-item{\n    display: flex;\n    line-height: 30px;\n    .item-content{\n      line-height: 30px;\n      height: auto;\n      width: 480px;\n    }\n    .item-title{\n      width: 80px;\n      height: 30px;\n      font-weight: bold;\n      color: #333;\n    }\n  }\n}\n</style>"],"sourceRoot":"src/views"}]}